@article{zwolferConciseNodalbasedDerivation2020a,
  title = {A Concise Nodal-Based Derivation of the Floating Frame of Reference Formulation for Displacement-Based Solid Finite Elements: {{Avoiding}} Inertia Shape Integrals},
  shorttitle = {A Concise Nodal-Based Derivation of the Floating Frame of Reference Formulation for Displacement-Based Solid Finite Elements},
  author = {Zw{\"o}lfer, Andreas and Gerstmayr, Johannes},
  year = {2020},
